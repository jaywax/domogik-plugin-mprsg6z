{
    "configuration": [],
    "device_types": {
        "mprsg6z.vamp": {
            "id": "mprsg6z.vamp",
            "description": "Mpr-Sg6z Virtual Amplifier",
            "name": "Virtual Amp",
            "sensors": ["channels"],
            "parameters": [
                {
		    "default": "My Vamp",
                    "key": "name",
		    "name": "Name",
                    "xpl": false,
                    "description": "The Mprsg6z name of the device",
		    "required": "yes",
                    "type": "string"
                },
                {
		    "default": "/dev/ttyUSB0",
                    "key": "device",
		    "name": "Device",
                    "xpl": false,
                    "description": "Serial bus to use to connect to the virtual Amp",
		    "required": "yes",
                    "type": "string"
                }
            ]
        },
        "mprsg6z.vzone": {
            "id": "mprsg6z.vzone",
            "description": "Mpr-Sg6z Virtual Zone",
            "name": "Virtual Zone",
            "commands": ["status", "mute", "donotdisturb", "volume", "treble", "bass", "balance", "channel"],
            "sensors": ["status", "mute", "donotdisturb", "volume", "treble", "bass", "balance", "channel"],
            "parameters": [
                {
		    "default": "My Vzone",
                    "key": "name",
		    "name": "Name",
                    "xpl": false,
                    "description": "The name of a vzone",
		    "required": "yes",
                    "type": "string"
                },
                {
		    "default": "11",
                    "key": "childs",
		    "name": "Childs",
                    "xpl": false,
                    "description": "Childs physical zone of a vamp. Must be separated by a coma, example : 11, 12, 13",
		    "required": "yes",
                    "type": "string"
                }
            ]
        }
    },
